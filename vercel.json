{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/login", "dest": "/frontend/login.html" },
    { "src": "/send-email", "dest": "/frontend/send-email.html" },
    { "src": "/(.*)", "dest": "/backend/server.js" }
  ]
}
